<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ct">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="at">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="ib.a" >
        <item name="android:minHeight">1dp</item>
    </style>

    <style name="ib" parent="@android:style/Holo.ButtonBar.AlertDialog">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="tl">
        <item name="android:stretchColumns">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="at.a">
        <item name="android:lineSpacingMultiplier">0.7</item>
    </style>
    <style name="at.b">
        <item name="android:lineSpacingMultiplier">0.8</item>
    </style>
    <style name="at.c">
        <item name="android:lineSpacingMultiplier">0.9</item>
    </style>

    <style name="textShadow1">
        <item name="android:shadowDy">2</item>
        <item name="android:shadowDx">2</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowColor">#ff000000</item>
    </style>

    <style name="textShadow2">
        <item name="android:shadowDy">3</item>
        <item name="android:shadowDx">3</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:shadowColor">#ff000000</item>
    </style>

    <style name="textShadow3">
        <item name="android:shadowDy">4</item>
        <item name="android:shadowDx">4</item>
        <item name="android:shadowRadius">5</item>
        <item name="android:shadowColor">#ff000000</item>
    </style>

    <style name="textShadow4">
        <item name="android:shadowDy">5</item>
        <item name="android:shadowDx">5</item>
        <item name="android:shadowRadius">10</item>
        <item name="android:shadowColor">#ff000000</item>
    </style>
</resources>